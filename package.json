{
  "name": "NineChronicles",
  "productName": "Nine Chronicles",
  "version": "0.1.0",
  "description": "Game Launcher for Nine Chronicles",
  "author": "Planetarium",
  "license": "GPL-3.0",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "hot"
  ],
  "homepage": "https://nine-chronicles.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planetarium/electron-launcher"
  },
  "bugs": {
    "url": "https://github.com/planetarium/electron-launcher/issues"
  },
  "main": "main.js",
  "scripts": {
    "build": "yarn codegen && webpack --env NODE_ENV=development --env target=render && webpack --env NODE_ENV=development --env target=main",
    "build-prod": "yarn codegen && webpack --env NODE_ENV=production --env target=render && webpack --env NODE_ENV=production --env target=main",
    "build-release": "yarn codegen && webpack --env NODE_ENV=production --env target=render --env release && webpack --env NODE_ENV=production --env target=main",
    "server": "npx webpack serve --env NODE_ENV=development --env target=render --hot",
    "start": "npx webpack --env NODE_ENV=development --env target=main && electron ./dist/.",
    "start:v2": "npx webpack --env NODE_ENV=development --env target=main && electron ./dist/. --v2",
    "pack": "run-script-os",
    "pack:windows": "npm run build-prod && pwsh -nop -ex bypass -noni -f scripts\\electron-pack.ps1",
    "pack:darwin": "npm run build-prod && scripts/electron-pack.sh",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run start\"",
    "dev:v2": "concurrently --kill-others \"npm run server\" \"npm run start:v2\"",
    "codegen": "graphql-codegen",
    "build-headless": "run-script-os",
    "build-headless:windows": "pwsh -nop -ex bypass -noni -f scripts\\build-headless.ps1",
    "build-headless:darwin": "scripts/build-headless.sh || true",
    "bundle-player": "npx ts-node scripts/downloadPlayer.ts",
    "sign-apv": "run-script-os",
    "sign-apv:windows": "powershell -nop -ex bypass -noni -f scripts\\sign-apv.ps1",
    "sign-apv:darwin": "scripts/sign-apv.sh || true",
    "latest-apv-no": "npx ts-node scripts/latestApvNo.ts",
    "configure-apv": "npx ts-node scripts/configureApv.ts",
    "before-pack": "npm install && npm run build-headless && npm run bundle-player && npm run sign-apv",
    "pack-all": "npm run before-pack && npm run pack",
    "pack-all:electron-builder": "run-script-os",
    "pack-all:electron-builder:windows": "yarn before-pack && electron-builder --windows",
    "pack-all:electron-builder:darwin": "yarn before-pack && electron-builder --mac",
    "pack-release": "npm run before-pack && npm run release",
    "release": "run-script-os",
    "release:windows": "npm run build-release && pwsh -nop -ex bypass -noni -f scripts\\electron-pack.ps1",
    "release:darwin": "npm run build-release && scripts/electron-pack.sh",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register __tests__/**/*.spec.ts --exit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "update-translations": "txjs-cli push src"
  },
