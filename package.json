{
  "name": "NineChronicles",
  "productName": "Nine Chronicles",
  "version": "0.1.0",
  "description": "Game Launcher for Nine Chronicles",
  "author": "Planetarium",
  "license": "MIT",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "hot"
  ],
  "homepage": "https://nine-chronicles.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planetarium/electron-launcher"
  },
  "bugs": {
    "url": "https://github.com/planetarium/electron-launcher/issues"
  },
  "main": "main.js",
  "scripts": {
    "build": "npm run codegen && npx webpack --env.NODE_ENV=development --env.target=render && npx webpack --env.NODE_ENV=development --env.target=main",
    "build-prod": "npm run codegen && npx webpack --env.NODE_ENV=production --env.target=render && npx webpack --env.NODE_ENV=production --env.target=main",
    "server": "npm run codegen && npx webpack-dev-server --env.NODE_ENV=development --env.target=render --hot",
    "start": "npm run codegen && npx webpack --env.NODE_ENV=development --env.target=main && npx electron ./dist/.",
    "pack": "npm run build-prod && npx electron-packager ./dist --out ./pack --overwrite --icon=./app.ico",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run start\"",
    "codegen": "graphql-codegen",
    "build-headless": "run-script-os",
    "build-headless:windows": "dotnet publish nekoyume-unity/NineChronicles.Standalone.Executable/NineChronicles.Standalone.Executable.csproj -r win-x64 -o dist/publish --self-contained",
    "build-headless:darwin": "dotnet publish nekoyume-unity/NineChronicles.Standalone.Executable/NineChronicles.Standalone.Executable.csproj -r osx-x64 -o dist/publish --self-contained",
    "bundle-player": "npx ts-node scripts/downloadPlayer.ts",
    "sign-apv": "run-script-os",
    "sign-apv:windows": "powershell -nop -ex bypass -noni -f scripts\\sign-apv.ps1 || (exit 0)",
    "sign-apv:darwin": "scripts/sign-apv.sh || true",
    "latest-apv-no": "npx ts-node scripts/latestApvNo.ts",
    "configure-apv": "npx ts-node scripts/configureApv.ts",
    "before-pack": "npm install && npm run build-headless && npm run bundle-player && npm run sign-apv",
    "pack-all": "npm run before-pack && npm run pack",
    "pack-all:electron-builder": "run-script-os",
    "pack-all:electron-builder:windows": "npm run before-pack && npm run build-prod && npx electron-builder --windows",
    "pack-all:electron-builder:darwin": "npm run before-pack && npm run build-prod && npx electron-builder --mac"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@sentry/electron": "^1.3.0",
    "@types/lockfile": "^1.0.1",
    "@types/mixpanel-browser": "^2.35.3",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-retry": "^2.2.16",
    "apollo-link-ws": "^1.0.20",
    "bencodex": "^0.1.1",
    "buffer": "^5.6.0",
    "child-process-promise": "^2.2.1",
    "electron-dl": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.1",
    "electron-store": "^5.2.0",
    "extract-zip": "^2.0.1",
    "file-loader": "^6.0.0",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "lockfile": "^1.0.4",
    "mixpanel-browser": "^2.38.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mobx-react-router": "^4.1.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-youtube": "^7.11.2",
    "subscriptions-transport-ws": "^0.9.16",
    "tmp-promise": "^3.0.2",
    "url-loader": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/typescript": "^1.15.4",
    "@graphql-codegen/typescript-operations": "^1.15.4",
    "@graphql-codegen/typescript-react-apollo": "^1.15.4",
    "@graphql-codegen/typescript-resolvers": "^1.16.3",
    "@planetarium/cli": "^0.9.5",
    "@sentry/cli": "^1.54.0",
    "@sentry/webpack-plugin": "^1.11.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/xml2js": "^0.4.5",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "electron": "^9.0.2",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-download": "^4.1.1",
    "electron-packager": "^14.2.1",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "run-script-os": "^1.1.1",
    "sass-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "xml2js": "^0.4.23"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
