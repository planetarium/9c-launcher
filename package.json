{
  "name": "NineChronicles",
  "productName": "Nine Chronicles",
  "version": "0.1.0",
  "description": "Game Launcher for Nine Chronicles",
  "author": "Planetarium",
  "license": "GPL-3.0",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "hot"
  ],
  "homepage": "https://nine-chronicles.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planetarium/electron-launcher"
  },
  "bugs": {
    "url": "https://github.com/planetarium/electron-launcher/issues"
  },
  "main": "main.js",
  "scripts": {
    "build": "npm run codegen && npx webpack --env.NODE_ENV=development --env.target=render && npx webpack --env.NODE_ENV=development --env.target=main",
    "build-prod": "npm run codegen && npx webpack --env.NODE_ENV=production --env.target=render && npx webpack --env.NODE_ENV=production --env.target=main",
    "build-release": "npm run codegen && npx webpack --env.NODE_ENV=production --env.target=render --env.release && npx webpack --env.NODE_ENV=production --env.target=main",
    "server": "npm run codegen && npx webpack-dev-server --env.NODE_ENV=development --env.target=render --hot",
    "start": "npm run codegen && npx webpack --env.NODE_ENV=development --env.target=main && npx electron ./dist/.",
    "pack": "run-script-os",
    "pack:windows": "npm run build-prod && pwsh -nop -ex bypass -noni -f scripts\\electron-pack.ps1",
    "pack:darwin": "npm run build-prod && scripts/electron-pack.sh",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run start\"",
    "codegen": "graphql-codegen",
    "build-headless": "run-script-os",
    "build-headless:windows": "pwsh -nop -ex bypass -noni -f scripts\\build-headless.ps1 || (exit 0)",
    "build-headless:darwin": "scripts/build-headless.sh || true",
    "bundle-player": "npx ts-node scripts/downloadPlayer.ts",
    "sign-apv": "run-script-os",
    "sign-apv:windows": "powershell -nop -ex bypass -noni -f scripts\\sign-apv.ps1 || (exit 0)",
    "sign-apv:darwin": "scripts/sign-apv.sh || true",
    "latest-apv-no": "npx ts-node scripts/latestApvNo.ts",
    "configure-apv": "npx ts-node scripts/configureApv.ts",
    "before-pack": "npm install && npm run build-headless && npm run bundle-player && npm run sign-apv",
    "pack-all": "npm run before-pack && npm run pack",
    "pack-all:electron-builder": "run-script-os",
    "pack-all:electron-builder:windows": "npm run before-pack && npm run build-prod && npx electron-builder --windows",
    "pack-all:electron-builder:darwin": "npm run before-pack && npm run build-prod && npx electron-builder --mac",
    "pack-release": "npm run before-pack && npm run release",
    "release": "run-script-os",
    "release:windows": "npm run build-release && pwsh -nop -ex bypass -noni -f scripts\\electron-pack.ps1",
    "release:darwin": "npm run build-release && scripts/electron-pack.sh",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register __tests__/**/*.spec.ts --exit"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@lifeomic/attempt": "^3.0.0",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@sentry/electron": "^1.3.0",
    "@types/axios": "^0.14.0",
    "@types/lockfile": "^1.0.1",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-retry": "^2.2.16",
    "apollo-link-ws": "^1.0.20",
    "axios": "^0.21.1",
    "bencodex": "^0.1.1",
    "buffer": "^5.6.0",
    "cancellationtoken": "^2.2.0",
    "check-disk-space": "^2.1.0",
    "child-process-promise": "^2.2.1",
    "electron-dl": "^3.0.2",
    "electron-fetch": "^1.7.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.1",
    "electron-store": "^5.2.0",
    "extract-zip": "^2.0.1",
    "file-loader": "^6.0.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "graphql-tag": "^2.10.3",
    "lockfile": "^1.0.4",
    "material-ui-popup-state": "^1.6.1",
    "mixpanel": "^0.13.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mobx-react-router": "^4.1.0",
    "pretty-bytes": "^5.3.0",
    "public-ip": "^4.0.3",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-youtube": "^7.11.3",
    "subscriptions-transport-ws": "^0.9.16",
    "textfit": "^2.4.0",
    "tmp-promise": "^3.0.2",
    "url-loader": "^4.1.0",
    "uuid": "^8.3.2",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/typescript": "^1.15.4",
    "@graphql-codegen/typescript-operations": "^1.15.4",
    "@graphql-codegen/typescript-react-apollo": "^1.15.4",
    "@graphql-codegen/typescript-resolvers": "^1.16.3",
    "@planetarium/cli": "^0.9.5",
    "@sentry/cli": "^1.54.0",
    "@sentry/webpack-plugin": "^1.11.1",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/child-process-promise": "^2.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.6",
    "@types/xml2js": "^0.4.5",
    "@types/youtube": "0.0.39",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "electron": "^9.0.2",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-download": "^4.1.1",
    "electron-packager": "^14.2.1",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.1.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "run-script-os": "^1.1.1",
    "sass-loader": "^8.0.2",
    "spectron": "^11.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "xml2js": "^0.4.23"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "tsc --noEmit"
    }
  }
}
