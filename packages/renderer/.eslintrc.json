{
  "env": {
    "browser": true,
    "node": false
  },
  "plugins": ["@emotion"],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    /** These rules are disabled because they are incompatible with prettier */
    "vue/html-self-closing": "off",
    "vue/singleline-html-element-content-newline": "off"
  }
}
